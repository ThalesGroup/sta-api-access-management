{
  "Logging": {
    "IncludeScopes": "false",
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "JwtAuthorizationAndAuthenticationEnabled": "false",
  "JwtAuthentication": {
    "ValidateIssuer": "true",
    "ValidateAudience": "true",
    "ClientId": "[YOUR_CLIENT_ID]",
    "Issuer": "[YOUR_ISSUER]",
    "Authority": "[YOUR_WELL_KNOWN_OPENID_CONFIGURATION]"
  }
}
